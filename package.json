{
  "name": "auto-gif-customer-v1",
  "version": "1.0.0-alpha.1",
  "description": "Command-Line Diffuser Lattice Matrix - Self-Resolving Visual Computation Engine",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "mkdir -p dist && cp index.html dist/",
    "build:vite": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .js,.svelte",
    "lint:fix": "eslint src --ext .js,.svelte --fix",
    "format": "prettier --write 'src/**/*.{js,svelte,css,md}'",
    "format:check": "prettier --check 'src/**/*.{js,svelte,css,md}'",
    "type-check": "svelte-check --tsconfig ./jsconfig.json",
    "validate": "npm run lint && npm run format:check && npm run test",
    "clean": "rimraf dist .svelte-kit",
    "optimize-assets": "node scripts/optimize-assets.js",
    "generate-docs": "node scripts/generate-docs.js",
    "deploy": "node scripts/deploy.js",
    "analyze": "vite-bundle-visualizer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/auto-gif-customer-v1.git"
  },
  "keywords": [
    "gif",
    "design-system",
    "category-theory",
    "visual-computation",
    "brand-consistency",
    "cli",
    "diffuser",
    "lattice",
    "convergence",
    "manifold",
    "svelte",
    "reactive",
    "self-resolving",
    "automation"
  ],
  "author": "Auto-GIF-Customer Contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/your-org/auto-gif-customer-v1/issues"
  },
  "homepage": "https://github.com/your-org/auto-gif-customer-v1#readme",
  "dependencies": {
    "svelte": "^4.2.8",
    "gifenc": "^1.0.3",
    "chroma-js": "^2.4.2",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.0.4",
    "bezier-easing": "^2.1.0",
    "gl-matrix": "^3.4.3"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@testing-library/svelte": "^4.0.5",
    "@vitest/ui": "^1.1.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-svelte": "^2.35.1",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "rimraf": "^5.0.5",
    "svelte-check": "^3.6.2",
    "vite": "^5.0.10",
    "vite-bundle-visualizer": "^1.0.0",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ]
}
